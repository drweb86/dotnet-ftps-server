  <ItemGroup>
    <Resource Include="FtpsApp.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Siarhei_Kuchuk.FtpsServerLibrary" Version="2025.12.2" />
  </ItemGroup>

</Project>
